{{#each book in model}}
<div>
<p>{{book.title}} by {{book.author}}</p>
<p>
{{#link-to 'books.show' book}} Show {{/link-to}} | 
{{#link-to 'books.edit' book}} Edit {{/link-to}} | 
<button {{action 'delete' book}}>Delete</button>
</p>
</div>
{{/each}}

<p> {{#link-to 'books.new'}}Add a Book{{/link-to}} </p>
